{
  "name": "react-chart-loading",
  "version": "1.0.2",
  "description": "Loading spinner for charts",
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es6",
    "build-cjs": "rimraf lib && babel ./modules -d lib --ignore '__specs__'",
    "build-es6": "rimraf es6 && babel ./modules -d es6 --blacklist=es6.modules --ignore '__specs__'",
    "start": "node examples/dev-server.js",
    "test": "echo \"No tests yet\"; exit 0"
  },
  "keywords": [
    "react",
    "chart",
    "loading",
    "spinner",
    "d3"
  ],
  "author": "jaketrent",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "d3": "^3.5.16",
    "koa": "^1.2.0",
    "koa-mount": "^1.3.0",
    "koa-static": "^2.0.0",
    "koa-webpack-dev-middleware": "^1.2.0",
    "postcss": "^5.0.19",
    "postcss-cssnext": "^2.5.2",
    "postcss-import": "^8.1.0",
    "postcss-url": "^5.1.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-styleable": "^2.2.3",
    "rf-release": "^0.4.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14"
  },
  "peerDependencies": {
    "d3": "^3.5.16",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-styleable": "^2.2.3"
  },
  "dependencies": {
    "lodash.random": "^3.1.4",
    "lodash.range": "^3.1.4"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaketrent/react-chart-loading.git"
  },
  "bugs": {
    "url": "https://github.com/jaketrent/react-chart-loading/issues"
  },
  "homepage": "https://github.com/jaketrent/react-chart-loading#readme"
}
